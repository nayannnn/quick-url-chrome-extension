{
  "manifest_version": 3,
  "name": "Quick bookmarks",
  "description": "Keeping track of frequently visited bookmarks made easy.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "Icons/bookmark.png",
      "48": "Icons/bookmark.png",
      "128": "Icons/bookmark.png"
    }
  },
  "icons": {
    "16": "Icons/bookmark.png",
    "48": "Icons/bookmark.png",
    "128": "Icons/bookmark.png"
  },
  "permissions": ["tabs", "favicon"]
}